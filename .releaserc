{
    "plugins": [
        [
            "@semantic-release/git",
            {
                "assets": [
                    "package.json",
                    "docs/CHANGELOG.md",
                    "npm-shrinkwrap.json"
                ]
            }
        ],
        [
            "@semantic-release/npm",
            {
                "pkgRoot": "lib"
            }
        ],
    ],
    "branches": [
        "master",
        {
            name: "develop",
            prerelease: true
        },
        {
            name: "develop-*",
            prerelease: true
        },
    ]
}
